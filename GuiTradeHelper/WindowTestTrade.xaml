<Window x:Class="SoloVova.TradeHelper.GuiTradeHelper.WindowTestTrade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:SoloVova.TradeHelper.GuiTradeHelper"
        mc:Ignorable="d"
        Title="WindowTestTrade" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Button Grid.Row="0" Grid.Column="0" Content="Refresh" Click="ButtonRefresh_OnClick" />
        <TextBox Grid.Row="1" x:Name="tb" Grid.Column="0" Grid.ColumnSpan="2" />
    </Grid>
</Window>