<Page x:Class="SoloVova.TradeHelper.GuiTradeHelper.PageDefs"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:SoloVova.TradeHelper.GuiTradeHelper"
      mc:Ignorable="d"
      Title="PageDefs" Height="Auto" Width="Auto"
      Loaded="PageDefs_OnLoaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="30" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Button Grid.Column="0" Grid.Row="0" Content="Save" Click="ButtonSave_OnClick"></Button>
        <Label Grid.Column="0" Grid.Row="1" Content="DefProduct"></Label>
        <TextBox Grid.Column="1" Grid.Row="1" x:Name="TbDefProduct"></TextBox>
    </Grid>
</Page>
